{
    "source": {
        "include": ["js"],
        "includePattern": ".js$",
        "excludePattern": " (node_modules/ |docs)"
    }, 
    "plugins": ["plugins/markdown"],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": true
    },
    "opts": {
        "sort":false,
        "recurse": true,
        "destination": "./docs/"
        
    }
}

