{
  "name": "keykaps",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "vite --host",
    "starto": "vite --host --open",
    "build": "vite build --watch",
    "preview": "vite preview --host",
    "deploy": "surge ./dist keykaps.surge.sh",
    "clean": "rm -rf ./dist ./node_modules ./package-lock.json",
    "sass": "sass ./scss:CSS --watch --style=compressed",
    "clean:maps": "find ./public -name '*.map' -delete",
    "doc": "jsdoc -c jsdoc.json",
    "buildIndex": "parcel build ./js/index.js --dist-dir public",
    "watchIndex": "parcel watch ./js/index.js --dist-dir public",
    "buildAdmin": "parcel build ./js/admin.js --dist-dir public",
    "watchAdmin": "parcel watch ./js/admin.js --dist-dir public",
    "buildLogin": "parcel build ./js/login.js --dist-dir public",
    "watchLogin": "parcel watch ./js/login.js --dist-dir public",
    "buildAddProducts": "parcel build ./js/addProducts.js --dist-dir public",
    "watchAddProducts": "parcel watch ./js/addProducts.js --dist-dir public"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "animate.css": "^4.1.1",
    "jsdoc": "^4.0.0",
    "swiper": "^8.4.5"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "^2.8.2",
    "sass": "^1.57.1"
  }
}
